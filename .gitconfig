[user]
	name = mini-gui
	email = guido.minieri@gmail.com
[alias]
	ll = log --oneline
    ss = status
    au = add -u
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = green
	changed = yellow
	untracked = red
[core]
	excludesfile = /Users/guido/.gitexcludes
[credential]
	helper = osxkeychain
